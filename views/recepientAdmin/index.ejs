        <div class="modal-body">

          <br><br>
          <!-- <h5 style="font-family: 'Montserrat', sans-serif; font-weight: 600;" id="line2">You have raised a request.
            </h5>
            <br><br> -->
          <center>
            <h1 style="font-family: 'Montserrat', sans-serif; font-weight: 300;" id="line1"><img src="./img/logo.png"
                width="65px" alt="">
              COVID FREE INDIA</h1>
          </center>
          <br><br>
          <div class="container">
            <form action="/posts-r">
              <div class="form-row">
                <div class="form-group col-md-6">
                  <label for="inputEmail4">Id type</label>
                  <input type="text" class="form-control" name="Id type" id="Id type" placeholder="ID type">
                </div>
                <div class="form-group col-md-6">
                  <label for="inputPassword4">ID number</label>
                  <input type="text" class="form-control" name="Id number" id="Id number" placeholder="ID number">
                </div>
              </div>
              <div class="form-group">
                <label for="inputState">Is person suffering from any other disease?</label>
                <select class="custom-select my-1 mr-sm-2" id="inlineFormCustomSelectPref">
                  <option slected value="">..</option>
                  <option value="language">Yes</option>
                  <option value="society">No</option>
                </select>
              </div>
              <div class="form-group">
                <label for="inputAddress2"></label>
                <input type="text" class="form-control" id="inputAddress2" placeholder="Please specify (if any)">
              </div>
              <div class="form-group">
                <label for="inputZip">Diagnosis report(optional)</label>
                <input type="file" id="inputZip">
              </div>
              <div class="form-row">
                <div class="form-group col-md-6">
                  <label for="inputCity">City</label>
                  <input type="text" class="form-control" id="inputCity">
                </div>
                <div class="form-group col-md-4">
                  <label for="inputState">Blood Group</label>
                  <select class="custom-select my-1 mr-sm-2" id="inlineFormCustomSelectPref">
                    <option slected value="">Blood Group</option>
                    <option value="language">A +ve</option>
                    <option value="language">A -ve</option>
                    <option value="society">B +ve</option>
                    <option value="language">B -ve</option>
                    <option value="language">AB +ve</option>
                    <option value="language">AB -ve</option>
                    <option value="language">O +ve</option>
                    <option value="language">O -ve</option>
                  </select>
                </div>
                <div class="form-group col-md-2">
                  <label for="inputState">Age</label>
                  <select class="custom-select my-1 mr-sm-2" id="inlineFormCustomSelectPref">
                    <option slected value="">Age</option>
                    <option value="language">Below 18</option>
                    <option value="society">18-45</option>
                    <option value="language">45-60</option>
                    <option value="language">Above 60</option>

                  </select>
                </div>
              </div>
              <div class="form-group">
                <div class="form-check">
                  <input class="form-check-input" type="checkbox" id="gridCheck">
                  <label for="agree-term" class="label-agree-term"><span><span></span></span>I agree to the <a href="#"
                      class="term-service">Terms and Conditions</a></label>
                </div>
              </div>
              <a href="/posts-r"><button class="btn my-2 my-sm-0 contact-nav" type="submit">
                  Submit </button></a>
              <a href="/user-r"><button class="btn btn-secondary" type="submit">
                  Save draft </button></a>
            </form>
          </div>
          <!-- <h5 style="font-family: 'Montserrat', sans-serif; font-weight: 300;" id="line2">You'll be hearing from the
              donors soon.</h5> -->

          <br><br>

